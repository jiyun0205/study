#include <iostream>
#include <algorithm>

int main()
{
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(NULL);

    int a, b, c;

    while(1)
    {
        std::cin >> a >> b >> c;

        if (a==0 && b==0 && c==0) break;

        int max = std::max({a, b, c});
        int sum_others = a + b + c - max;

        if (sum_others <= max) std::cout << "Invalid\n";
            else if (a==b && b==c && a==c) std::cout << "Equilateral\n";
            else if (a==b || b==c || a==c) std::cout << "Isosceles\n";
            else std::cout << "Scalene\n";
        }

    return 0;
}